    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="sync, find" type="Boolean" array="False" size=""/>
            <declare name="name" type="String" array="False" size=""/>
            <declare name="mode, number" type="Integer" array="False" size=""/>
            <call expression="logger(&quot;str&quot;, plugins(&quot;ask name&quot;, &quot;&quot;))"/>
            <call expression="delay(2)"/>
            <input variable="name"/>
            <call expression="namecorrection(name)"/>
