                                        </then>
                                        <else/>
                                    </if>
                                </else>
                            </if>
                        </then>
                        <else>
                            <comment text="Normal Oyun Modu"/>
                            <declare name="index, index2, think" type="Integer" array="False" size=""/>
                            <assign variable="index" expression="0"/>
                            <assign variable="index2" expression="0"/>
                            <assign variable="sync" expression="true"/>
                            <assign variable="number" expression="rand(false)"/>
                            <call expression="logger(&quot;str&quot;, plugins(&quot;get num&quot;, name))"/>
                            <call expression="delay(2)"/>
                            <comment text="Say&#305; Belirlendi"/>
                            <while expression="sync">
                                <if expression="index == 10">
                                    <then>
                                        <assign variable="find" expression="false"/>
                                        <assign variable="sync" expression="false"/>
                                        <comment text="10 Hak &#304;&#231;inden Tahmin Edilemez ise D&#246;ng&#252;y&#252; Kapat"/>
                                    </then>
                                    <else>
                                        <assign variable="index2" expression="index2 + 1"/>
                                        <assign variable="index" expression="index + 1"/>
                                        <if expression="index == 1">
                                            <then>
                                                <comment text="&#304;lk ba&#351;lama mesaj&#305;"/>
                                                <call expression="logger(&quot;str&quot;, plugins(&quot;think&quot;, name))"/>
                                            </then>
                                            <else>
                                                <comment text="Sonraki ba&#351;lama mesajlar&#305;"/>
                                                <if expression="index == 10">
                                                    <then>
                                                        <comment text="Son tahmin hakk&#305; i&#231;in bilgilendirme mesaj&#305;"/>
                                                        <call expression="logger(&quot;str&quot;, plugins(&quot;sthink&quot;, name))"/>
                                                        <call expression="delay(3.5)"/>
                                                    </then>
                                                    <else>
                                                        <comment text="Yanl&#305;&#351; tahminler i&#231;in bilgilendirme mesaj&#305;"/>
                                                        <call expression="logger(&quot;str&quot;, plugins(&quot;wthink&quot;, name))"/>
                                                    </else>
                                                </if>
                                            </else>
                                        </if>
                                        <input variable="think"/>
                                        <if expression="think == number">
                                            <then>
                                                <comment text="Tahmin Do&#287;ru &#304;se `find` bool'u true olarak ayarla ve d&#246;ng&#252;y&#252; kapat."/>
                                                <assign variable="find" expression="true"/>
                                                <assign variable="sync" expression="false"/>
                                            </then>
                                            <else>
                                                <comment text="Tahmin Yanl&#305;&#351; &#304;se D&#246;ng&#252;y&#252; Yeniden Ba&#351;lat"/>
                                            </else>
                                        </if>
                                    </else>
                                </if>
                            </while>
                            <if expression="find">
                                <then>
                                    <comment text="Oyun Kazan&#305;ld&#305;&#287;&#305;nda Buraya D&#246;n"/>
                                    <if expression="index2 &lt; 4">
                                        <then>
                                            <comment text="Say&#305; hemen tahmin edilirse ek mesaj g&#246;nder"/>
                                            <call expression="logger(&quot;str&quot;, plugins(&quot;swin&quot;, name))"/>
                                            <call expression="delay(1)"/>
                                            <call expression="logger(&quot;str&quot;, plugins(&quot;win&quot;, name))"/>
                                            <call expression="delay(2)"/>
                                            <call expression="logger(&quot;str&quot;, index2 &amp; &quot;. Denemede Oyunu Kazand&#305;n&#305;z!&quot;)"/>
                                        </then>
                                        <else>
                                            <comment text="Klasik tebrik mesaj&#305;"/>
                                            <call expression="logger(&quot;str&quot;, plugins(&quot;win&quot;, name))"/>
                                            <call expression="delay(2)"/>
                                            <call expression="logger(&quot;str&quot;, index2 &amp; &quot;. Denemede Oyunu Kazand&#305;n&#305;z!&quot;)"/>
                                        </else>
                                    </if>
                                </then>
                                <else>
                                    <comment text="Oyun Kaybedildi&#287;inde Buraya D&#246;n"/>
                                    <call expression="logger(&quot;str&quot;, plugins(&quot;lose&quot;, name))"/>
                                    <call expression="delay(2)"/>
                                    <call expression="logger(&quot;str&quot;, &quot;Bir kez daha oynamak i&#231;in 1 yazman yeterli.&quot;)"/>
                                    <input variable="name"/>
                                    <if expression="name == &quot;1&quot;">
                                        <then>
                                            <comment text="Program kapanmadan bir kez daha oynama hakk&#305; verir."/>
