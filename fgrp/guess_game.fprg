    <function name="rand" type="Integer" variable="payload">
        <parameters>
            <parameter name="type" type="Boolean" array="False"/>
        </parameters>
        <body>
            <declare name="payload, opt, num" type="Integer" array="False" size=""/>
            <declare name="index" type="Integer" array="True" size="101"/>
            <comment text="100 yerine 101 kullanmam&#305;z&#305;n nedeni array'lar&#305;n (dizin) 0. konumdan ba&#351;lamas&#305;d&#305;r.&#13;&#10;&#13;&#10;var dizin = [&quot;bir&quot;, &quot;iki&quot;, &quot;&#252;&#231;&quot;]   &#13;&#10;return dizin[0]     // &quot;bir&quot;"/>
            <if expression="!type">
                <then>
                    <assign variable="payload" expression="Random(101)"/>
                    <comment text="De&#287;i&#351;ken kontrol&#252; istemeyenler i&#231;in altyap&#305;s&#305; while ile kurulu (flowgorithm i&#231;inde varsay&#305;lan olarak bulunur) Random fonksiyonu."/>
                    <comment text="De&#287;i&#351;ken kontrol&#252; i&#231;in;  &#13;&#10;rand(true)    &#13;&#10;&#13;&#10;Direk i&#351;lem i&#231;in;  &#13;&#10;rand(false)   &#13;&#10;&#13;&#10;kullan&#305;n."/>
                    <if expression="payload == 0">
                        <then>
                            <call expression="rand(false)"/>
                        </then>
                        <else/>
                    </if>
                </then>
                <else>
                    <comment text="Bu k&#305;s&#305;m de&#287;i&#351;ken kontrol&#252; yapmak isteyenler i&#231;indir."/>
                    <assign variable="num" expression="0"/>
                    <do expression="num &lt; 101">
                        <assign variable="index[num]" expression="num"/>
                        <assign variable="num" expression="num + 1"/>
                    </do>
                    <comment text="do d&#246;ng&#252;s&#252; kullan&#305;l&#305;r."/>
                    <assign variable="opt" expression="Random(101)"/>
                    <assign variable="payload" expression="index[opt]"/>
                    <if expression="payload == 0">
                        <then>
                            <call expression="rand(true)"/>
                        </then>
                        <else/>
                    </if>
                </else>
            </if>
            <comment text="&#304;ndexler 0'a e&#351;it olursa, sistem otomatik olarak ba&#351;a d&#246;ner ve say&#305; tekrardan kar&#305;&#351;t&#305;r&#305;l&#305;r."/>
            <comment text="&#199;&#305;k&#305;&#351; int (Tamsay&#305;) olarak d&#246;ner."/>
        </body>
    </function>
