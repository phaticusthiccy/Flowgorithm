            <comment text="&#304;sim Alma K&#305;sm&#305; Tamamland&#305;"/>
            <call expression="logger(&quot;str&quot;, plugins(&quot;after name&quot;, name))"/>
            <call expression="delay(9)"/>
            <call expression="logger(&quot;str&quot;, plugins(&quot;game mode&quot;, &quot;&quot;))"/>
